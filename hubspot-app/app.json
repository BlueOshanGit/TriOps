{
  "name": "TriOps",
  "description": "Execute custom code and webhooks in HubSpot workflows",
  "uid": "triops",
  "scopes": [
    "crm.objects.contacts.read",
    "crm.objects.contacts.write",
    "crm.objects.companies.read",
    "crm.objects.deals.read",
    "automation"
  ],
  "public": true,
  "extensions": {
    "crm": {
      "cards": []
    }
  },
  "auth": {
    "redirectUrls": [
      "https://reena-noncorroborating-readily.ngrok-free.dev/oauth/callback",
      "http://localhost:3100/oauth/callback"
    ],
    "requiredScopes": [
      "crm.objects.contacts.read",
      "automation"
    ],
    "optionalScopes": [
      "crm.objects.contacts.write",
      "crm.objects.companies.read",
      "crm.objects.deals.read"
    ]
  },
  "webhooks": {
    "hapikey": false
  },
  "settingsPages": {
    "configurationPage": {
      "targetUrl": "https://reena-noncorroborating-readily.ngrok-free.dev/settings"
    }
  }
}
